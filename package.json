{
  "name": "data-annotation",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "seed": "npx tsc -p tsconfig.build.json & TZ='UTC' node dist/helpers/seed.js",
    "build": "npx tsc -p tsconfig.build.json",
    "start": "TZ='UTC' node dist/index.js",
    "dev": "npx tsc -p tsconfig.build.json --watch & TZ='UTC' nodemon dist/index.js",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "lint": "eslint . --report-unused-disable-directives",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "docker:dev:up": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up -d && docker logs data-annotation-dev -f",
    "docker:dev:down": "docker compose -f docker-compose.yml -f docker-compose.dev.yml down",
    "docker:test:up": "docker compose -f docker-compose.yml -f docker-compose.test.yml up -d && docker logs data-annotation-test -f",
    "docker:test:down": "docker compose -f docker-compose.yml -f docker-compose.test.yml down"
  },
  "keywords": [
    "express-typescript",
    "data-annotation",
    "mongodb"
  ],
  "author": "Minh Tri Pham",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.3",
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/module-alias": "^2.0.2",
    "@types/morgan": "^1.9.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.40.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.0.1",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "joi": "^17.10.1",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment": "^2.29.4",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "rate-limit-redis": "^4.0.0",
    "redis": "^4.6.8",
    "reflect-metadata": "^0.1.13",
    "typescript": "~5.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@types/jest": "^29.5.5",
    "@types/supertest": "^2.0.14",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --ignore-unknown --write"
    ]
  }
}
